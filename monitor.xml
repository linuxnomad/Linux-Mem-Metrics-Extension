<monitor>
    <name>LinuxMemMonitor</name>
       <type>managed</type>
        <description>Monitors Memory Stats with "free" command.</description> 
        <monitor-configuration>     </monitor-configuration>
        <monitor-run-task>
           <!--  Edit execution-style as needed. -->
           <execution-style>periodic</execution-style>
           <name>Run</name>
           <type>executable</type>
           <task-arguments></task-arguments>
            <executable-task>
                <type>file</type>
                 <file os-type="linux">linux-mem_v01.sh</file>
           </executable-task>
         </monitor-run-task>
</monitor>
